<script language="javascript">
		var countdown = localStorage.getItem("countdown3");
		if(null == countdown || '' == countdown) {
			countdown = parseInt(new Date().getTime()/1000)+3689;
			localStorage.setItem("countdown3",countdown);
		}else{
			var current_countdown = parseInt(new Date().getTime()/1000);
			if(current_countdown > countdown){
				countdown = parseInt(new Date().getTime()/1000)+3689;
				localStorage.setItem("countdown3",countdown);
			}
		}
		addTimeLesser(1,countdown);	
		$("#countdown").show();
</script>

<script type="text/javascript">
	var payoid = ""; 
	function checkInput(){
		
		$('#subbtn').attr("disabled","disabled");
		
		if($("input[name=pattern]:checked").length < 1){
			return shom_msg('请选择一个产品款式');
		}
		
		if($("input[name=sex]:checked").length < 1){
			return shom_msg('请选择一个产品尺码');
		}		
		if($("input[name=attr]:checked").length < 1){
				return shom_msg('请选择一个产品其它，可自定义');
		}		
		if ($('#name').val()=='') {
			return shom_msg('收货人姓名不能为空');
		}
		
		var rname =  $.trim($('#name').val());
		var reg=/^[\u4e00-\u9fa5]+$/;
		 if (!reg.test(rname)){
			return shom_msg('姓名格式不正确，必须为中文');
    	}
		if(rname.length > 4){
			return shom_msg('姓名不能大于4个字符');
		}
		
		if ($('#mobile').val()=='') {
			return shom_msg('手机号码不能为空');
		}	
			
		if(!checkMobile($('#mobile').val())){
			return shom_msg('手机号码有误，请重填');
		}
		
		var province = $("select[name='province'] option:selected").text();
		var city = $("select[name='city'] option:selected").text();
		var district = $("select[name='district'] option:selected").text();
		
		if(province=='- 省/自治区/直辖市 -' || city=='- 市 -' || province=='' || city=='' || district=='- 区/县 -' || district==''){
			return shom_msg('请填写正确的省市区');
		}
			
		if ($('#detailaddress').val()=='') {
			return shom_msg('详细街道地址不能为空');
		}	

		if ($('#detailaddress').val().length < 4) {
			return shom_msg('街道地址少于4个字符,请填写详细');
		}	
		
		
		 		
		if(payoid != ""){
				window.location.href = "http://www.cm322.cn/weixinpay/wxpay.html?aid=3&oid="+payoid;
				return false;
		}		 
		 var jz = layer.open({
			type: 2
			,content: '提交中',
			shadeClose: false
		  });

		 $.ajax({
					type : "POST",  //提交方式
					url : "/index.php/home/mobile/orderAdd",			
					data : $("#form1").serialize(),
					dataType : "json",
					success : function(result) {
						if (result.status=="0"){
							layer.close(jz);						
													payoid = result.oid;
							window.location.href = "http://www.cm322.cn/weixinpay/wxpay.html?aid=3&oid="+result.oid;						
						}else{
							layer.close(jz);
							$('#subbtn').removeAttr("disabled");
							layer.open({content: result.msg,btn: '我知道了'});
						}
					},error:function(){
							layer.close(jz);/public/static/css/
							$('#subbtn').removeAttr("disabled");
							layer.open({content: '您的网络不稳定，请关闭页面后重试！',btn: '我知道了'});
					}
				});
	
				
	}
	
	//验证必须是正确的手机号
	function checkMobile(val){
		var reg =/^1[3456789]\d{9}$/;
		if(reg.test(val)){
			return true;
		}else{
			return false;
		}
	}
	
	//提示信息 1为默认 2为弹框
	function shom_msg(msg){
		$('#subbtn').removeAttr("disabled");
		layer.open({content: msg,skin: 'msg',time: 2 });
		return false;
	}
</script>

<script>
