	function sphone(){
		var mobile = $('#ssphone').val();
		if(!checkMobile(mobile)){
			layer.open({content: "查询的手机号码有误，请重填",skin: 'msg',time: 2 });
		}else{
			window.location.href="http://www.k11y.cn/aftersale/express2.html?mobile="+mobile;
		}
	}
	
		function comp(d){
			var number = $("#number").val();
			var price = $("#price").val();
			number =  parseInt(number);
			price =  parseFloat(price);
			if(d == "1"){
				if(number > 1 ){
					number = number - 1;
				}else{
					layer.open({content: '最少必须选择一件',skin: 'msg',time: 2 });
				}
			}else{
				if(number >= 10 ){
					layer.open({content: '最多不能超过10件',skin: 'msg',time: 2 });
				}else{
					number = number + 1;
				}
			}			
						
			$("#number").val(number);
			var showprice = (price * number).toFixed(2);
			$("#showprice").html("¥ "+showprice);
		}
	

	var userList = ["胡 * * 河南省 郑州 * * ","王 * * 吉林省 松原 * * ","李 * * 广东省 东莞 * * ","张 * * 重庆市 万州 * * ","王 * * 河南省 南阳 * * ","刘 * * 湖北省 武汉 * * ","龙 * * 广东省 中山 * * ","宋 * * 贵州省 铜仁 * * ","鄢 * * 湖北省 武汉 * * ","赵 * * 江苏省 徐州 * * ",
					"吕 * * 河南省 郑州 * * ","李 * * 河北省 石家 * * ","池 * * 上海市 浦东 * * ","杨 * * 广东省 汕头 * * ","张 * * 河北省 石家 * * ","王 * * 贵州省 贵阳 * * ","陈 * * 黑龙江省 哈 * * ","张 * * 天津市 静海 * * ","张 * * 河北省 石家 * * ","魏 * * 黑龙江省 哈 * * ","谢 * * 河北省 石家 * * ","王 * * 河北省 衡水 * * ","刘 * * 北京市 顺义 * * ","孙 * * 河北省 石家 * * ",
					"李 * * 河北省 衡水 * * ","李 * * 上海市 黄浦 * * ","孙 * * 河北省 石家 * * ","谢 * * 河北省 石家 * * ","赵 * * 河南省 郑州 * * ","王 * * 河南省 开封 * * "];
	var html = "";
	$.each(userList,function(i,item){
		var myDate = new Date();    
		var h=myDate.getHours();
		var m=myDate.getMinutes();
		var s=myDate.getSeconds();
		if(m > 5){m = m - Math.floor(Math.random()*4);}
　　	if(s > 15){s = s - Math.floor(Math.random()*9);}
		if(s < 10){s = '0' + s;}				
		if(m < 10){m = '0' + m;}
		if(h == 0){h = 12;}
		html += '<li>'+userList[Math.floor(Math.random()*30)]+' '+h+':'+m+':'+s+' 下单成功 </li>';
　　});	
	$("#gd").append(html);
	$("#leftmenu").show();
	
    function noticeUp(obj,top,time) {
        $(obj).animate({
            marginTop: top
        }, time, function () {
            $(this).css({marginTop:"0"}).find(":first").appendTo(this);
        })
    }
    $(function () {
        // 调用 公告滚动函数
        setInterval("noticeUp('.notice ul','-28px',500)", 3000);
    });

    $(document).ready(function () {
        var menuYloc = $("#leftmenu").offset().top; //此ID为随着屏幕滚动div的ID
        $(window).scroll(function () {
            var offsetTop = menuYloc + $(window).scrollTop() + "px";
            $("#leftmenu").animate({ top: offsetTop }, { duration: 600, queue: false }); //此ID为随着屏幕滚动div的ID
			
			//底部btn列表-显示隐藏
			var sc = $(document).scrollTop();
			var hh = $(document).height() - $(window).height();
			var cha = hh - sc;
			if(cha >= 1300){
				$("#btn-list").removeClass('fade-out');
			}else{
				$("#btn-list").addClass('fade-out');
			}
        });
    });
</script>
